#include <iostream>
using namespace std;
bool isPowerOfTwo(int n) {
    if (n == 0 || n == 1)
        return true;
    if (n%2==0) {
        return isPowerOfTwo(n/2);
    }else
        return false;
}
int main() {
    int n;
    cin>>n;
    if (isPowerOfTwo(n))
        cout<<"true";
    else
        cout<<"false";
}
