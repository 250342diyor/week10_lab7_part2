#include <iostream>
using namespace std;
int HowManyZero(int n) {
    if (n != 0) {
        if (n%10 == 0) {
            return HowManyZero(n/10) + 1;
        }else if (n%10 != 0) {
            return HowManyZero(n/10);
        }
    }
    return 0;
}
int main() {
    int n;
    cin >> n;
    cout << HowManyZero(n) << endl;
}
