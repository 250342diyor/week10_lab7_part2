#include <iostream>
using namespace std;

bool incseq(int n) {
    if (n < 10)
        return true;
    if (n % 10 <= (n / 10) % 10)
        return false;
    return incseq(n / 10);
}

int main() {
    int n;
    cin >> n;
    if (incseq(n))
        cout << "true" << endl;
    else
        cout << "false" << endl;
}
